@model IEnumerable<MVCTravelBookingISE.Models.AccomodationModel>

@{
    ViewData["Title"] = "Index";
}

<h1>List of Accomodations</h1>
<p>The destination, ratings and price of accomodation is listed below</p>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Destination)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Rooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Bathrooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Distance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Rate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acco_Price)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Destination)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Rooms)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Bathrooms)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Distance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Rate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acco_Price)
            </td>
          
        </tr>
}
    </tbody>
</table>
