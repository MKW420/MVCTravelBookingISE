@model AccomodationModel

@{
    ViewData["Title"] = "Update Accomodation";
}


<h4>Rate the accomodation</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
           
            <div class="form-group">
                <label asp-for="Acco_Rate" class="control-label"></label>
                <input asp-for="Acco_Rate" class="form-control" />
                <span asp-validation-for="Acco_Rate" class="text-danger"></span>
            </div>
          
        
           
            <div class="form-group">
                <input type="submit" value="Submit Rating" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
